programa
{
	// Exercício 01
	
	funcao inicio()
	{
		inteiro a, b, c
		a = 2
		b = 5
		c = 10

		escreva("Letra A: ", a+b*c/a,"\n")
		escreva("Letra B: ", (a+b)*c/a, "\n")
		escreva("Letra C: ", (a+b*c)/a)
	}
}
